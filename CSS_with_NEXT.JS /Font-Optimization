import { Roboto } from 'next/font/google';
const roboto=Roboto({
  weight:"100",
  subsets:["latin"],
  display:"swap"
});

export default function Home() {

  return (
    <div >
      <p className={roboto.className}> remember to use roboto.className correctly in your 
        JSX to apply the font style. If you're using CSS modules, it will automatically map roboto.
        className to the correct CSS class generated by Next.js for the font styles.</p>



    </div>
  );
}


